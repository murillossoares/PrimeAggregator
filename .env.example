# Required
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=
SOLANA_COMMITMENT=confirmed
WALLET_SECRET_KEY=

# Bot
MODE=dry-run

# default | hft
# - hft: reduz logs (LOG_VERBOSE default=false) e restringe OpenOcean para evitar ban/rate-limit.
BOT_PROFILE=default
CONFIG_PATH=./config.json
POLL_INTERVAL_MS=500
HEALTHCHECK_PORT=0
BALANCE_REFRESH_MS=2000

# Dynamic amountA (optional; only applies when pair.aMint == SOL)
# Uses wallet SOL balance (minus MIN_BALANCE_LAMPORTS) * (bps/10000) as amountA override.
DYNAMIC_AMOUNT_A_MODE=off
DYNAMIC_AMOUNT_A_BPS=0
DYNAMIC_AMOUNT_A_MIN_ATOMIC=0
DYNAMIC_AMOUNT_A_MAX_ATOMIC=0
EXECUTION_STRATEGY=atomic
# NOTE:
# - OpenOcean execution requires `EXECUTION_STRATEGY=sequential`
# - Ultra is sequential (2 tx) and requires pairs with `aMint=So111...` (SOL)
# immediate | avg-window | vwap | bollinger
TRIGGER_STRATEGY=immediate
TRIGGER_OBSERVE_MS=30000
TRIGGER_OBSERVE_INTERVAL_MS=1000
TRIGGER_EXECUTE_MS=10000
TRIGGER_EXECUTE_INTERVAL_MS=500
TRIGGER_BOLLINGER_K=1.5
TRIGGER_EMA_ALPHA=0
TRIGGER_BOLLINGER_MIN_SAMPLES=10
TRIGGER_MOMENTUM_LOOKBACK=2
TRIGGER_TRAIL_DROP_BPS=1
TRIGGER_EMERGENCY_SIGMA=0
TRIGGER_AMOUNT_MODE=fixed
TRIGGER_MAX_AMOUNTS_PER_TICK=1
DRY_RUN_BUILD=true
DRY_RUN_SIMULATE=false
DRY_RUN_INCLUDE_JITO_TIP=false
LIVE_PREFLIGHT_SIMULATE=true

# Advanced: sequential confirmation (mainly affects Ultra/OpenOcean sequential)
SEQUENTIAL_CONFIRM_MAX_ATTEMPTS=4
SEQUENTIAL_CONFIRM_BASE_DELAY_MS=200
PRIORITY_FEE_STRATEGY=off
PRIORITY_FEE_LEVEL=recommended
PRIORITY_FEE_REFRESH_MS=1000
PRIORITY_FEE_MAX_MICRO_LAMPORTS=50000000
PRIORITY_FEE_TARGET_ACCOUNT_LIMIT=16
PRIORITY_FEE_WITH_JITO=false
HELIUS_API_KEY=
HELIUS_RPC_URL=https://mainnet.helius-rpc.com
LOG_PATH=./logs/events.jsonl
# true = loga todos os candidates; false = loga apenas candidates lucrativos e pula eventos grandes (simulate)
LOG_VERBOSE=true
# Optional: rotate JSONL log file by size (0 disables)
LOG_ROTATE_MAX_BYTES=10485760
LOG_ROTATE_MAX_FILES=5
BASE_FEE_LAMPORTS=5000
RENT_BUFFER_LAMPORTS=0
COMPUTE_UNIT_LIMIT=1400000
COMPUTE_UNIT_PRICE_MICRO_LAMPORTS=0
QUOTE_CACHE_TTL_MS=250
FEE_CONVERSION_CACHE_TTL_MS=60000
LUT_CACHE_TTL_MS=60000
PAIR_CONCURRENCY=2
# Spreads pair scans across `POLL_INTERVAL_MS` to avoid bursty HTTP (recommended)
PAIR_SCHEDULER_SPREAD=true
MIN_BALANCE_LAMPORTS=0
MAX_ERRORS_BEFORE_EXIT=0
MAX_CONSECUTIVE_ERRORS_BEFORE_EXIT=0
AUTO_SETUP_WALLET=false

# Jito (optional, for MODE=live)
JITO_ENABLED=false
JITO_BLOCK_ENGINE_URL=https://amsterdam.mainnet.block-engine.jito.wtf
JITO_TIP_LAMPORTS=10000
JITO_TIP_MODE=fixed
JITO_MIN_TIP_LAMPORTS=5000
JITO_MAX_TIP_LAMPORTS=50000
JITO_TIP_BPS=2000
JITO_WAIT_MS=0
JITO_FALLBACK_RPC=false
JITO_TIP_ACCOUNT=

# Jupiter
# Quote base URL used for scanning and fee conversion (SOL -> aMint).
# Defaults to `JUP_SWAP_BASE_URL`.
JUP_QUOTE_BASE_URL=
JUP_SWAP_BASE_URL=https://api.jup.ag

# Ultra Swap (optional; requires API key)
# Base URL for Ultra. Both of these work:
# - https://api.jup.ag
# - https://api.jup.ag/ultra
JUP_ULTRA_BASE_URL=https://api.jup.ag
JUP_API_KEY=
# Execution provider:
# - swap  = build/execute via Jupiter swap APIs (supports atomic + sequential)
# - ultra = execute via Ultra (sequential only; can be combined with OpenOcean in the same bot)
JUP_EXECUTION_PROVIDER=swap
# Backward-compat: if true, implies `JUP_EXECUTION_PROVIDER=ultra` (prefer the var above)
JUP_USE_ULTRA=false
JUP_MIN_INTERVAL_MS=150
JUP_RPS=
JUP_BURST=1
JUP_ADAPTIVE_PENALTY_MS=120000
# Per-pair circuit breaker cooldown when hitting HTTP 429
JUP_429_COOLDOWN_MS=30000
JUP_BACKOFF_MAX_ATTEMPTS=4
JUP_BACKOFF_BASE_MS=250
JUP_BACKOFF_MAX_MS=5000
# Optional Ultra-specific rate limit overrides (defaults to the values above)
JUP_ULTRA_MIN_INTERVAL_MS=
JUP_ULTRA_BACKOFF_MAX_ATTEMPTS=
JUP_ULTRA_BACKOFF_BASE_MS=
JUP_ULTRA_BACKOFF_MAX_MS=

# OpenOcean (optional; public API has strict rate limit ~1-2 RPS and may ban on 429)
OPENOCEAN_ENABLED=false
# Tip: the bot normalizes this value, so you can also pass it without `https://`
# (e.g. `open-api.openocean.finance/v4/solana`)
OPENOCEAN_BASE_URL=https://open-api.openocean.finance/v4/solana
OPENOCEAN_API_KEY=
OPENOCEAN_GAS_PRICE=5
OPENOCEAN_MIN_INTERVAL_MS=1200
OPENOCEAN_RPS=
OPENOCEAN_BURST=2
OPENOCEAN_ADAPTIVE_PENALTY_MS=180000
# Per-pair circuit breaker cooldown when hitting HTTP 429 / bans
OPENOCEAN_429_COOLDOWN_MS=60000
OPENOCEAN_SIGNATURES_ESTIMATE=3
# NOTE: OpenOcean execution requires EXECUTION_STRATEGY=sequential
# Optional: restrict OpenOcean routing by dex index (see `/v4/solana/dexList`; eg Jupiter=6, Titan=10)
OPENOCEAN_ENABLED_DEX_IDS=
OPENOCEAN_DISABLED_DEX_IDS=
# Optional: tracking/fee program (note: `referrerFee` reduces your profitability)
OPENOCEAN_REFERRER=
OPENOCEAN_REFERRER_FEE=
OPENOCEAN_OBSERVE_ENABLED=false
OPENOCEAN_EXECUTE_ENABLED=true
OPENOCEAN_EVERY_N_TICKS=2
OPENOCEAN_JUPITER_GATE_BPS=-250
# Only query OpenOcean when Jupiter best is within [gate, gate+near] bps (set 0 to disable)
OPENOCEAN_JUPITER_NEAR_GATE_BPS=50

# Optional hybrid mode: use Rust decision engine (requires building `rust/arb_calc`)
USE_RUST_CALC=false
RUST_CALC_PATH=./target/release/arb_calc
